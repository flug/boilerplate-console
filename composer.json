{
    "name": "Clooder/console-boilerplate",
    "description": "a starter project for application cli",
    "type": "project",
    "require": {
        "symfony/console": "^4.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "symfony/filesystem": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/config": "^4.0",
        "symfony/process": "^4.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Flug",
            "email": "flugv1@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clooder\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0",
        "phpspec/phpspec": "^6.0"
    }
}
